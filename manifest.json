{
  "name": "Happy Slack",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hide messages from specific users, make Slack A Happy Place Again!",
  "homepage_url": "http://ketanbhatt.github.io/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.slack.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}